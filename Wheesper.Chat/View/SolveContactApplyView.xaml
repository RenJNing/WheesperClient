<UserControl x:Class="Wheesper.Chat.View.SolveContactApplyView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wheesper.Chat.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="accept" HorizontalAlignment="Center" Margin="78,164,82,0" VerticalAlignment="Top" Width="140" Command="{Binding AcceptContactApplyCommod, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}" Style="{DynamicResource ButtonStyle1}" Height="28"/>
        <Button Content="reject" HorizontalAlignment="Center" Margin="78,232,82,0" VerticalAlignment="Top" Width="140" Command="{Binding RejectContactApplyCommod, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}" Style="{DynamicResource ButtonStyle1}" Height="28"/>
        <Button Content="close" HorizontalAlignment="Center" Margin="78,95,82,0" VerticalAlignment="Top" Width="140" Command="{Binding CloseSolveContactApplyCommond, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True}" Style="{DynamicResource ButtonStyle1}" Height="28"/>
    </Grid>
</UserControl>
